<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="native_gantt.Controller" owl="1">
        <div t-att-class="className" t-ref="root">
            <Layout display="props.display" className="'o_view_sample_data'">
                <div t-att-class="'o_native_gantt_view'">

                     <t t-set-slot="layout-buttons">
                     </t>

                    <t t-set-slot="layout-actions">
                        <SearchBar/>
                    </t>

                    <t t-component="props.Renderer"
                       model="model"
                       controller="this"
                    />
                </div>
            </Layout>
        </div>
    </t>


</templates>